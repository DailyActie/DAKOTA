# ESM is an experimental capability
# This is a regression test only, not a verification test

environment
  tabular_data

method,
  efficient_subspace
    model_pointer = 'SUBSPACE'
    seed = 24620
    # evals in the subspace
    samples = 100
  output debug

model                             
  subspace
    id_model = 'SUBSPACE'
    actual_model_pointer = 'FULLSPACE'
    initial_samples  100
      refinement_samples 2
    max_iterations = 10
    convergence_tolerance = 1.0e-3
#   truncation_method bing_li         #s1
#   truncation_method constantine     #s2

model 
  single
  id_model = 'FULLSPACE'
    variables_pointer = 'ID_V'  
    interface_pointer = 'ID_I'  
    responses_pointer = 'ID_R'  

variables,
  id_variables = 'ID_V'
  normal_uncertain = 100
    means = 100*0.5
    std_deviations = 100*0.2

interface,
  id_interface = 'ID_I'
  direct
      analysis_driver = 'aniso_quad_form'
    analysis_components = 'seed:61043' 'eigenvals: 302.56 134.2 53.9 5.8 2.1'

responses,
  id_responses = 'ID_R'
  num_response_functions = 1
  analytic_gradients
  no_hessians
