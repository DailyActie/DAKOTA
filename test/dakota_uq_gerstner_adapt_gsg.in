environment,
#	output_precision = 16

method,
	polynomial_chaos				#0,#1,#2,#3,#4,#5
#	stoch_collocation				#6,#7,#8,#9,#10,#11
	  dimension_adaptive p_refinement generalized
	    convergence_tol = 1.e-10
