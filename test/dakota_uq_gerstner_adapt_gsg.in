#	  analysis_components = 'aniso1'		#3,#9
#	  analysis_components = 'aniso2'		#4,#10
#	  analysis_components = 'aniso3'		#5,#11
#	  deactivate restart_file evaluation_cache

responses,
        response_functions = 1
        no_gradients
        no_hessians
