variables,
        continuous_design = 2
	  initial_point      =    5.      15.
	  descriptors        =   'b'      'h'
	normal_uncertain = 2
	  means              =  500.0   2000.0
	  std_deviations     =  100.0    400.0
	  descriptors        =   'P'      'M'
	lognormal_uncertain = 1
       	  means             =    5.0
	  std_deviations    =    0.5
	  descriptors       =    'Y'
	uncertain_correlation_matrix =  1   0.5 0
					0.5 1   0
					0   0   1

interface,
	system asynch
	  analysis_driver = 'short_column'

responses,
	response_functions = 2
	analytic_gradients
