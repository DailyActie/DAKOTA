#@ s*: ReqFiles=surrogate_build_pts.dat,surrogate_build_pts_ff.dat
## DAKOTA INPUT FILE - dakota_surrogate_import.in

environment
          tabular_graphics_data
          method_pointer = 'UQ'
 
method,
        id_method = 'UQ'
        model_pointer = 'SURR'
        output verbose 
        sampling 
        sample_type lhs 
        samples = 100
        seed = 5034
                 
model,                                         
        id_model = 'SURR'                        
        surrogate global,                        
        import_points_file = 'surrogate_build_pts.dat' annotated #s0,#s2
#        import_points_file = 'surrogate_build_pts_ff.dat' freeform #s1,#s3
        polynomial quadratic                   
        challenge_points_file = 'surrogate_build_pts.dat' annotated #s0,#s3
#        challenge_points_file = 'surrogate_build_pts_ff.dat' freeform #s1,#s2


variables,
        active all
	discrete_design_range = 1
	  initial_point      3
          lower_bounds       0
          upper_bounds       6
          descriptors      'x1'
        discrete_design_set
          integer = 1
	    initial_point = 6
            set_values = 2 4 6 8 10
            descriptors      'x2'
          real = 1
            set_values = 1. 3. 5. 7. 9.
            descriptors      'x3'
        binomial_uncertain = 1
          prob_per_trial    =  0.3
          num_trials        =  10
          descriptors       =  'x4'
        uniform_uncertain = 2
          upper_bounds = 1.4 1.5
          lower_bounds = 0.2 0.3
          descriptors      'u1' 'u2'
        discrete_state_set
          integer = 1
            initial_state = 2
            set_values = 2 4 6 8 10
            descriptors      'x4'
          real = 1
            initial_state = 1.
            set_values = 1. 3. 5. 7. 9.
            descriptors      'x5'

interface,
	direct
	  analysis_driver = 'text_book'

responses,
	response_functions = 3
	no_gradients
	no_hessians
