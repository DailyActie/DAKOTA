# DAKOTA INPUT FILE : dakota_cantilever_ego.in
# Note: currently this test file performs three different tests:
# (1) efficient_global for constrained, global opt. with mean random vars
# (2) efficient_global for constrained, global NLS  with mean random vars
# (3) global_reliability for UQ at the nominal design

# timeout overrides: 0=TD120, 1=TD120, 2=TD300

method,
	efficient_global					#0,#1
#	global_reliability x_gaussian_process		#2
#	  num_response_levels = 0 2 2				#2
#	  response_levels = 0.2 0.6 0.2 0.6			#2
	  seed = 123457

variables,
	continuous_design = 2
	  initial_point    4.0          4.0			#0,#1
#	  initial_point    2.5          2.5			#2
	  upper_bounds    10.0         10.0
	  lower_bounds     1.0          1.0
	  descriptors     'w' 't'
        continuous_state = 4					#0,#1
          initial_state   40000.  29.E+6  500.  1000.	#0,#1
	  descriptors       'R'    'E'    'X'    'Y'	#0,#1
#	normal_uncertain = 4					#2
#	  means             =  40000. 29.E+6 500. 1000.	#2
#	  std_deviations    =  2000. 1.45E+6 100. 100.	#2
#	  descriptors       =  'R' 'E' 'X' 'Y'		#2

interface,
	direct
	  analysis_driver = 'cantilever'

responses,
	num_objective_functions = 1				#0
	num_nonlinear_inequality_constraints = 2		#0
#	num_least_squares_terms = 3				#1
#       num_response_functions = 3				#2
	no_gradients
	no_hessians
