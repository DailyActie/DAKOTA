# DAKOTA INPUT FILE - dakota_cantilever_dream.in
# p0=1


method,									
        bayes_calibration queso, 	#0,#p0						
         #NOTE:  typically you will want tens of thousands of samples, this is just a short test
         samples = 1000 seed = 348
          mcmc_type dram
          rejection delayed
          metropolis adaptive
          #output debug
	  # 1. diagonal with values
	  proposal_covariance diagonal
	    values 1.0e6 1.0e-1
	  # 2. matrix with values
#	  proposal_covariance matrix
#	    values 1.0 0.1
#                  0.1 2.0
	  # 3. diagonal or matrix from file
#	  proposal_covariance diagonal  # OR matrix
#	    filename 'mycov.dat'

variables,								
	active uncertain
        uniform_uncertain 2
          upper_bounds  1.e8 10.0
#  Truth value is 2.85e7 2.5 
          lower_bounds 1.e6 0.1
          initial_point 2.85e7 2.5
          descriptors 'E' 'w' 
        continuous_state 4 
          initial_state 3 40000 500 1000 
          descriptors 't' 'R' 'X' 'Y' 

interface,								
        system 
          analysis_driver = 'cantilever3'

responses,								
	calibration_terms = 2
        calibration_data_file = 'dakota_cantilever_queso.withsigma.dat'
          freeform
          num_experiments = 10
	  # read two scalar sigma values in each row
	  variance_type = 'scalar'
  	descriptors = 'stress' 'displacement'
 	no_gradients							
	no_hessians
