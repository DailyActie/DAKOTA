# DAKOTA INPUT FILE - dakota_uq_subspace.in
# Contains regression tests for using model subspace
# with several methods

environment
  output_precision = 16
  method_pointer = 'ID_METHOD'

# Tests 0-9 are for sampling methods:
method,
  id_method = 'ID_METHOD'
  sampling                          #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s17
    model_pointer = 'SUBSPACE'      #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s17
    seed = 24620                    #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s17
    samples = 100                   #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s17
    sample_type random              #s0,#s1,#s2,#s3,#s4,#s17
#   sample_type lhs                 #s5
#   sample_type incremental_lhs     #s6,#s7
#   sample_type incremental_random  #s8,#s9
#     refinement_samples = 100      #s6,#s8
#     refinement_samples = 100 200  #s7,#s9
# polynomial_chaos                  #s10,#s11,#s12,#s13,#s14,#s15
#   model_pointer = 'SUBSPACE'      #s10,#s11,#s12,#s13,#s14,#s15
#   sparse_grid_level = 3           #s13
#   quadrature_order = 2            #s14
#   expansion_order   = 6           #s10,#s11,#s12
#   orthogonal_least_interpolation  #s15
#   collocation_ratio = 0.5         #s12
#   collocation_points = 100        #s10,#s11,#s15
#   omp                             #s10,#s11,#s12
#   basis_type                      #s10,#s11,#s12
#     adapted                       #s10,#s11,#s12
#   seed = 1234567                  #s10,#s11,#s12,#s13,#s14,#s15
# local_reliability                 #s16
#   model_pointer = 'SUBSPACE'      #s16
  output debug

# Tests 0-4 test the various subspace input parameters
# with method sampling
model                             
  subspace
    id_model = 'SUBSPACE'
    actual_model_pointer = 'FULLSPACE'
    initial_samples  100
      refinement_samples 2
    max_iterations = 10
    convergence_tolerance = 1.0e-3
#   truncation_method bing_li         #s1
#   truncation_method constantine     #s2,#s11,#s12,#s13,#s14,#s15,#s16
#   truncation_method                 #s17
#     energy                          #s17
#       truncation_tolerance 1e-6     #s17
#   dimension 5                       #s3
#   bootstrap_samples 150             #s4
#   dimension 15                      #s10

model 
  single
  id_model = 'FULLSPACE'
    variables_pointer = 'ID_V'  
    interface_pointer = 'ID_I'  
    responses_pointer = 'ID_R'  

variables,
  id_variables = 'ID_V'
  normal_uncertain = 100              #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
    means = 100*0.5                   #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
    std_deviations = 100*0.2          #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
# normal_uncertain = 15               #s10
#   means = 15*0.                     #s10
#   std_deviations = 15*1.            #s10
# discrete_state_set string 1         #s10
#   set_values = "exponential"        #s10

interface,
  id_interface = 'ID_I'
  direct
    analysis_driver = 'aniso_quad_form'                                         #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
    analysis_components = 'seed:61043' 'eigenvals: 302.56 134.2 53.9 5.8 2.1'   #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
#   analysis_driver = 'diffusion_1d'	                                          #s10

responses,
  id_responses = 'ID_R'
  num_response_functions = 1
  analytic_gradients          #s0,#s1,#s2,#s3,#s4,#s5,#s6,#s7,#s8,#s9,#s11,#s12,#s13,#s14,#s15,#s16,#s17
# numerical_gradients         #s10
  no_hessians
