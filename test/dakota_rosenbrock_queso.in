method,									
        bayes_calibration queso,	#s0,#s1,#s2,#s3,#s4
         samples = 2000 seed = 348
         mcmc_type dram
           rejection delayed
#	   metropolis adaptive	# adapt from provided proposal covar
# PCE specs (sparse grid and regression)
	  emulator pce						#s0,#s1
	    sparse_grid_level = 2				#s0
#	    expansion_order = 4 collocation_ratio = 0.9		#s1
# SC spec (sparse grid and regression)
#	  emulator sc						#s2
#	    sparse_grid_level = 2				#s2
# GP specs (surfpack and dakota)
#	  emulator gaussian_process				#s3,#s4
#	    surfpack						#s3
#	    dakota						#s4
#	    emulator_samples = 30				#s3,#s4
	  output verbose
	    proposal_updates = 50
#	    posterior_adaptive

variables,								
        uniform_uncertain 2
          upper_bounds   2.  2.
          lower_bounds  -2. -2.
          initial_point -1.  1.
	  descriptors     'x1' 'x2'

interface,								
        direct 
          analysis_driver = 'rosenbrock'

responses,								
	calibration_terms = 2
        calibration_data_file = 'dakota_rosenbrock_queso.withsigma.dat'
          freeform
          num_experiments = 1
	  # read two scalar sigma values in each row
	  variance_type = 'scalar'
  	descriptors = 'f1' 'f2'
 	no_gradients							
	no_hessians
