#@ *: DakotaConfig=HAVE_QUESO,DAKOTA_HAVE_MPI
#@ p*: MPIProcs=1

method,									
	bayes_calibration queso
	  samples = 2000 seed = 348
	  mcmc_type dram
            rejection delayed
#	    metropolis adaptive	# adapt from provided proposal covar
	  emulator
	    pce sparse_grid_level = 2				#p0,#p7
#	    pce expansion_order = 4 collocation_ratio = 0.9	#p1
#	    pce expansion_order = 4 collocation_ratio = 1.2	#p6
#	    sc  sparse_grid_level = 2				#p2
#	    gaussian_process surfpack emulator_samples = 30	#p3,#p5
#	    gaussian_process dakota   emulator_samples = 30	#p4
	    proposal_updates = 50
#	    posterior_adaptive					#p5,#p6
#	    convergence_tolerance = .05				#p6
#	  output verbose

variables,								
	uniform_uncertain 2		#p0,#p1,#p2,#p3,#p4,#p5,#p6
	  upper_bounds   2.  2.		#p0,#p1,#p2,#p3,#p4,#p5,#p6
	  lower_bounds  -2. -2.		#p0,#p1,#p2,#p3,#p4,#p5,#p6
#	normal_uncertain 2  		#p7
#	  means          0.  0.		#p7
#	  std_deviations 1.  1.		#p7
	  initial_point -1.  1.
	  descriptors  'x1' 'x2'

interface,								
        direct 
          analysis_driver = 'rosenbrock'

responses,								
	calibration_terms = 2
        calibration_data_file = 'dakota_rosenbrock_queso.withsigma.dat'
          freeform num_experiments = 1 variance_type = 'scalar'
  	descriptors = 'f1' 'f2'
 	no_gradients							
	no_hessians
