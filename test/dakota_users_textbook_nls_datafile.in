# Dakota Input File: textbook_nls_datafile.in
method
    nl2sol                                 #0,#1
#    coliny_ea                              #2
#    seed = 3247                            #2              
    max_iterations = 50                     
    convergence_tolerance = 1e-8            

variables
  continuous_design = 2
    initial_point    0.9    1.1
    upper_bounds     5.8    2.9
    lower_bounds     0.5   -2.9
    descriptors      'x1'   'x2'

interface
 fork 
    analysis_driver = 'text_book'
    parameters_file = 'text_book.in'
    results_file    = 'text_book.out'
    file_tag #file_save

responses
   calibration_terms = 3
     calibration_data_file = 'dakota_users_textbook_nls_datafile.lsq.dat'       #0
#     calibration_data_file = 'dakota_users_textbook_replicates.lsq.dat'        #1,#2
#      num_experiments = 2                                                      #1,#2
#      num_replicates = 1 3                                                     #1,#2
       freeform
  analytic_gradients
  no_hessians
