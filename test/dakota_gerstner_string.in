# This input file tests the string variable capability by using a
# string variable to specify the analysis component for anisotropy
# level to Gerstner.

environment,
        tabular_graphics_data
#	output_precision = 16

method,
        multidim_parameter_study	#0
        partitions = 3 3 2		#0
#        sampling			#1
#        sample_type lhs 		#1
#        samples = 60			#1
#        seed = 5837			#1

variables,
        active all
	uniform_uncertain = 2
	  lower_bounds      0.  0.
	  upper_bounds      1.  1.
	  descriptors =    'x' 'y'
        discrete_uncertain_set string = 1
          elements_per_variable = 3
          elements = 'iso1' 'iso2' 'iso3'
          #elements = 'iso1' 'iso2' 'iso3' 'aniso1' 'aniso2' 'aniso3'
          set_probabilities = 1. 1. 1.   
          descriptors = 'ancomp'

interface,
        fork asynchronous evaluation_concurrency = 4
          analysis_driver = 'gerstner_string'
          parameters_file = 'params.in'
          results_file = 'results.out'
          file_tag

responses,
        response_functions = 1
        no_gradients
        no_hessians
