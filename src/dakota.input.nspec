              [ press {N_mom(true,pressFlag)} ]
              ]
	    [ challenge_points_file STRING {N_mom(str,approxChallengeFile)} 
              [ annotated {N_mom(true,approxChallengeAnnotated)} | 
                freeform  {N_mom(false,approxChallengeAnnotated)} ]
              ]
	    )
	  |
	  ( ( multipoint {N_mom(lit,surrogateType_multipoint_tana)}
	      tana {0}
	      )
	    |
	    ( local {N_mom(lit,surrogateType_local_taylor)}
