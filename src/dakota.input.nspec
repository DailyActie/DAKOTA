	        [ export_model_file STRING {N_mom(str, approxExportModelFile)} ]
	        )
              )
	    |
	    ( mars {N_mom(lit,surrogateType_global_mars)}
	      [ max_bases INTEGER {N_mom(shint,marsMaxBases)} ]
