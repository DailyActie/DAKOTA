		probabilities {N_mdm(type,responseLevelTarget_PROBABILITIES)}
		| reliabilities {N_mdm(type,responseLevelTarget_RELIABILITIES)}
		| gen_reliabilities {N_mdm(type,responseLevelTarget_GEN_RELIABILITIES)}
