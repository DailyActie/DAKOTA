# DAKOTA INPUT FILE - dakota_rosenbrock_multidim_paramstudy.in
# Included DAKOTA example using the rosenbrock analysis code in a constrained
# gradient-based optimization
# usage dakota -i dakota_rosenbrock_multidim_paramstudy.in -o Rosenbrock_multidim_paramstudy.out > Rosenbrock_multidim_paramstudy.stdout


strategy
  graphics
  tabular_graphics_data
    tabular_graphics_file = 'Rosenbrock_multidim_paramstudy.dat'
  single_method

method
  multidim_parameter_study
    partitions = 8 8

model
  single

variables
  continuous_design = 2
    lower_bounds    -2.0     -2.0
    upper_bounds     2.0      2.0
    descriptors      'x1'     "x2"

interface
  analysis_driver = 'rosenbrock'
    direct

responses
  response_functions = 1
  no_gradients
  no_hessians

